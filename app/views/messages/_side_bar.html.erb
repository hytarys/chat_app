<div class = "side_bar_top">
  <div class = "user_name">
    <%= link_to edit_user_path(current_user) do %>
      <%= current_user.name %>
    <% end %>
  </div>
  <div class = "create_chat">
    <a href = "/rooms/new">チャットを作成する</a>
  </div>
</div>
<div class = "side_bar_main">
  <% current_user.rooms.each do |room|%>
    <div class= "chatroom">
      <div class ="chatroom-name">
        <a href="#"><%= room.name%></a>
      </div>
    </div>
  <%end%>
</div>